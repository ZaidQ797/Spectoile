{
  "name": "SpecEtoile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "openXcode": "cd ios; open SpecEtoile.xcworkspace",
    "pod_installer": "npx pod-install ios",
    "iPhone6s": "react-native run-ios --simulator \"iPhone 6s\"",
    "ios_staging_iphone_6s": "yarn iPhone6s --scheme StagingBookKaru --configuration StagingDebug",
    "iPhone11ProMax": "npx react-native run-ios --simulator \"iPhone 11 Pro Max\"",
    "podCleaner": "cd ios; pod deintegrate; rm -rf Podfile.lock; cd .. ; yarn pod_installer",
    "android_run_staging": "yarn addFlipperReduxIn; npx react-native run-android --variant=stagingDebug",
    "android_run_production": "npx react-native run-android --variant=productionDebug",
    "android_bundle_production": "cd android; ./gradlew bundleProductionRelease && cd app/build/outputs/bundle/productionRelease && cp -R ./ ~/Desktop && cd ~/Desktop && open .;",
    "android_bundle_staging": "yarn shotFlipperAway; cd android; ./gradlew bundleStagingRelease && cd app/build/outputs/bundle/stagingRelease && cp -R ./ ~/Desktop && cd ~/Desktop && open .;",
    "release_build_android_staging": "yarn shotFlipperAway; cd android && ./gradlew assembleStagingRelease && cd app/build/outputs/apk/staging/release && cp -R ./ ~/Desktop && cd ~/Desktop && open .;",
    "release_build_android_production": "cd android && ./gradlew assembleProductionRelease && cd app/build/outputs/apk/production/release && cp -R ./ ~/Desktop && cd ~/Desktop && open .;",
    "clean_android_cache": "cd android; ./gradlew clean;",
    "shotFlipperAway": "yarn remove redux-flipper react-native-flipper",
    "addFlipperReduxIn": "yarn add redux-flipper react-native-flipper",
    "rnuc_staging": "$(npm bin)/rnuc .env.staging.yaml",
    "rnuc_production": "$(npm bin)/rnuc .env.yaml",
    "gradlewClean": "cd android && ./gradlew clean && cd ..",
    "assembleRelease": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release",
    "bundleRelease": "cd android && ./gradlew bundleRelease && open app/build/"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.1.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/google-signin": "^5.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.9",
    "@react-native-firebase/admob": "^11.2.0",
    "@react-native-firebase/app": "^11.2.0",
    "@react-navigation/drawer": "^5.9.3",
    "@react-navigation/material-top-tabs": "^5.3.4",
    "@react-navigation/native": "^5.7.6",
    "@react-navigation/stack": "^5.9.3",
    "axios": "^0.21.0",
    "currency-symbol-map": "https://github.com/muhammadashfaq/currency-symbol-map.git",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-awesome-alerts": "^1.4.2",
    "react-native-collapsible": "^1.5.3",
    "react-native-credit-card-input": "https://github.com/muhammadashfaq/react-native-credit-card-input.git",
    "react-native-device-info": "^7.0.2",
    "react-native-dropdown-picker": "^3.7.1",
    "react-native-elements": "^2.3.2",
    "react-native-fbsdk": "^3.0.0",
    "react-native-geocoding": "^0.5.0",
    "react-native-geolocation-service": "^5.0.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-google-places-autocomplete": "^2.1.2",
    "react-native-google-recaptcha-v2": "^1.0.4",
    "react-native-i18n": "^2.0.15",
    "react-native-iap": "^5.2.14",
    "react-native-image-crop-picker": "^0.35.2",
    "react-native-image-picker": "^3.1.0",
    "react-native-keyboard-accessory": "^0.1.14",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-maps": "^0.27.1",
    "react-native-modal": "^11.5.6",
    "react-native-paper": "^4.4.0",
    "react-native-payments": "^0.8.3",
    "react-native-read-more-text": "^1.1.2",
    "react-native-reanimated": "^1.13.2",
    "react-native-render-html": "^4.2.4",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-snackbar": "^2.2.4",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-swipeout": "^2.3.6",
    "react-native-swiper": "^1.6.0",
